<style>
.container{
  background-color: #000;
}
.max-con {
    max-width: 1200px;
    margin: 0 auto;
    display: flex;
    justify-content: space-between;
}  

.store-info{
  display: flex;
  padding: 0px;
   list-style: none;
  color: white;
}
.tel-main{
  display: flex;
  align-item: center;
} 

.tel-main {
    display: flex;
    align-items: center;
    padding: 0px 10px;
}

a.tel-main svg {
    width: 20px;
    height: 20px;
}

.tel-main span{
      padding-left: 5px;
}

.sign-sec{
  display: flex;
  align-items: center;
  padding: 0px;
  color: white;
  list-style: none;
}  

.sign-sec li {
    padding: 0 10px;
}
.sche-cal {
    background-color: #404aa7;
    border-radius: 50px;
  padding: 6px 15px;
}
@media(max-width: 1100px){
  .tel-main span{
    display: none;
  }
}
@media(max-width: 768px){
  .right-section{
    display: none;
  }
  .max-con{
    display: flex;
    justify-content: center;
  }
}
  
</style>                         
